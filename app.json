{
  "expo": {
    "name": "scena",
    "slug": "tonight-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F9FA"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.scena.app",
      "infoPlist": {
        "NSCameraUsageDescription": "scena needs camera access to capture moments",
        "NSPhotoLibraryUsageDescription": "scena needs photo library access to share from your library",
        "NSLocationWhenInUseUsageDescription": "scena needs location access to tag where you are"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F8F9FA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.scena.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "scena needs camera access to capture moments"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "scena needs location to tag where you are"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "scena needs access to share from your library"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4b2be6d8-d112-4b2a-9606-d40033918c65"
      }
    },
    "owner": "henryvantieghem"
  }
}
